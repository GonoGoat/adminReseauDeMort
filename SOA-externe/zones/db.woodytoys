#/etc/bind/zones
;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.wt1-9.ephec-ti.be. admin.wt1-9.ephec-ti.be. (
                              5         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

; Name servers
wt1-9.ephec-ti.be.    IN      NS      ns.wt1-9.ephec-ti.be.

; A records for name servers
ns             IN      A       172.17.0.2

; Other A records
@               IN      A       172.17.0.3
www             IN      A       172.17.0.3
b2b             IN      A       172.17.0.3

